package main

import (
	"fmt"
)

func main() {
	var num1, num2, num3 float64
	fmt.Print("Digite sua primeira nota:")
	fmt.Scan(&num1)
	fmt.Print("Digite sua segunda nota:")
	fmt.Scan(&num2)
	fmt.Print("Digite sua terceira nota:")
	fmt.Scan(&num3)
	media_ponderada := (num1*2 + num2*3 + num3*5) / 10
	fmt.Println("a media ponderada sera: ", media_ponderada)
}
